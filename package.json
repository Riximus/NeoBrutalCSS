{
  "name": "neobrutalcss",
  "version": "0.6.4",
  "author": "Riximus",
  "description": "A simple and flexible CSS framework for building modern websites in the Neobrutalist style.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Riximus/NeoBrutalCSS.git"
  },
  "bugs": "https://github.com/Riximus/NeoBrutalCSS/issues",
  "keywords": [
    "neobrutalism",
    "css-framework",
    "web-design",
    "modern-css",
    "neobrutal-css",
    "ui-framework",
    "responsive-design",
    "web-development",
    "frontend",
    "styling",
    "minimalist",
    "aesthetic",
    "creative-design",
    "brutalist",
    "custom-css",
    "css",
    "neobrutalcss",
    "scss",
    "sass",
    "storybook",
    "neobrutalism",
    "brutalism",
    "colorful",
    "flexible"
  ],
  "main": "dist/neobrutal.css",
  "files": [
    "dist/neobrutal.css"
  ],
  "scripts": {
    "serve": "npx serve",
    "watch:sass": "sass --no-source-map --watch src/styles/main.scss:compiled/styles.css",
    "format:sass": "prettier --write \"src/**/*.scss\"",
    "compile:sass": "sass --no-source-map src/styles/main.scss:compiled/styles.css",
    "bundle:css": "lightningcss --browserslist --minify --bundle compiled/styles.css --output-file dist/neobrutal.css",
    "build": "npm run format:sass && npm run compile:sass && npm run bundle:css",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook --browsers firefox chromium webkit --no-cache",
    "watch:test-storybook": "test-storybook --watch",
    "build-storybook": "storybook build",
    "prod:storybook": "npm run compile:sass && npm run build-storybook"
  },
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/html": "^7.6.6",
    "@storybook/html-vite": "^7.6.6",
    "@storybook/test": "^7.6.6",
    "@storybook/test-runner": "^0.16.0",
    "lightningcss-cli": "^1.22.1",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.6"
  }
}
